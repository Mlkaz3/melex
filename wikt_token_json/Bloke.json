[{"meta": {"id": "bloke", "uuid": "1df9ee31-602e-48b4-971a-e16fd18855a1", "sort": "020286400", "src": "collegiate", "section": "alpha", "stems": ["bloke", "blokes"], "offensive": false}, "hwi": {"hw": "bloke", "prs": [{"mw": "\u02c8bl\u014dk", "sound": {"audio": "bloke001", "ref": "c", "stat": "1"}}]}, "fl": "noun", "ins": [{"il": "plural", "if": "blokes"}], "def": [{"sls": ["chiefly British", "informal"], "sseq": [[["sense", {"dt": [["text", "{bc}{sx|man|man:1|1a(1)}, {sx|fellow||4c} "], ["vis", [{"t": "But he himself was no ordinary {wi}bloke{/wi}.", "aq": {"auth": "Ian Templeton"}}, {"t": "\u2026 always knew a {wi}bloke{/wi} who knew a {wi}bloke{/wi} who had something or other on the highest authority.", "aq": {"auth": "Tim Winton"}}, {"t": "\u2026 I'd never go out with a {wi}bloke{/wi} to whom I hadn't been properly introduced.", "aq": {"auth": "Dillie Keane"}}, {"t": "Surely a {wi}bloke{/wi} is entitled to have his drink made the way he wants it.", "aq": {"auth": "E. Grisdale"}}]]]}]]]}], "et": [["text", "origin unknown"]], "date": "circa 1829", "shortdef": ["man, fellow"]}]