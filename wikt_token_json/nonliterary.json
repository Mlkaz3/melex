[{"meta": {"id": "nonliterary", "uuid": "1452588f-c2be-4a08-a641-5d0a564fccd6", "sort": "141907000", "src": "collegiate", "section": "alpha", "stems": ["nonliterary"], "offensive": false}, "hwi": {"hw": "non*lit*er*ary", "prs": [{"mw": "\u02ccn\u00e4n-\u02c8li-t\u0259-\u02ccrer-\u0113", "sound": {"audio": "nonliterary_1", "ref": "owl", "stat": "1"}}]}, "fl": "adjective", "def": [{"sseq": [[["sense", {"dt": [["text", "{bc}not {d_link|literary|literary} "], ["vis", [{"t": "In the work of novelists who have tasted other occupations, one generally finds an inverse correlation over the years between degree of literary success and degree of contact with {wi}nonliterary{/wi} reality.", "aq": {"auth": "James Lardner"}}, {"t": "{wi}nonliterary{/wi} sources"}]]]}]]]}], "date": "1850", "shortdef": ["not literary"]}]