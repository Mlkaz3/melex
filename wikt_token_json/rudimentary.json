[{"meta": {"id": "rudimentary", "uuid": "4ac4e3da-1757-444b-9012-238dca9e01fd", "sort": "184191000", "src": "collegiate", "section": "alpha", "stems": ["rudimentarily", "rudimentariness", "rudimentarinesses", "rudimentary"], "offensive": false}, "hwi": {"hw": "ru*di*men*ta*ry", "prs": [{"mw": "\u02ccr\u00fc-d\u0259-\u02c8men-t\u0259-r\u0113", "sound": {"audio": "rudime03", "ref": "c", "stat": "1"}}, {"mw": "-\u02c8men-tr\u0113"}]}, "fl": "adjective", "def": [{"sseq": [[["sense", {"sn": "1", "dt": [["text", "{bc}consisting in first {d_link|principles|principle} {bc}{sx|fundamental||} "], ["vis", [{"t": "had only a {wi}rudimentary{/wi} formal education", "aq": {"auth": "D. J. Boorstin"}}]]]}]], [["sense", {"sn": "2", "dt": [["text", "{bc}of a primitive kind "], ["vis", [{"t": "the equipment of these past empire-builders was {wi}rudimentary{/wi}", "aq": {"auth": "A. J. Toynbee"}}]]]}]], [["sense", {"sn": "3", "dt": [["text", "{bc}very imperfectly developed or represented only by a {d_link|vestige|vestige} "], ["vis", [{"t": "the {wi}rudimentary{/wi} tail of a hyrax"}]]]}]]]}], "uros": [{"ure": "ru*di*men*tar*i*ly", "prs": [{"mw": "\u02ccr\u00fc-d\u0259-\u02ccmen-\u02c8ter-\u0259-l\u0113", "sound": {"audio": "rudime04", "ref": "c", "stat": "1"}}, {"mw": "-\u02c8men-tr\u0259-l\u0113"}], "fl": "adverb"}, {"ure": "ru*di*men*ta*ri*ness", "prs": [{"mw": "\u02ccr\u00fc-d\u0259-\u02c8men-t\u0259-r\u0113-n\u0259s", "sound": {"audio": "rudime05", "ref": "c", "stat": "1"}}, {"mw": "-\u02c8men-tr\u0113-"}], "fl": "noun"}], "quotes": [{"t": "A few were hopeless imbeciles, unable to comprehend more than the {qword}rudimentary{/qword} requirements of filling their bellies when food was placed before them \u2026", "aq": {"auth": "Edgar Rice Burroughs", "source": "{it}The Monster Men{/it}", "aqdate": "1929"}}, {"t": "Despite the {qword}rudimentary{/qword} nature of its instruments\u2014an octant, a chronometer and a level\u2014the Lewis and Clark Expedition was a triumph for science and natural history.", "aq": {"auth": "Daniel J. Boorstin", "source": "{it}U.S. News & World Report{/it}", "aqdate": "11 July 1994"}}, {"t": "Because now, every schlemiel with a pair of mirrored sunglasses and a {qword}rudimentary{/qword} grasp of the rules of poker thinks he can play cards with the pros.", "aq": {"auth": "A. J. Jacobs", "source": "{it}New York Times Book Review{/it}", "aqdate": "8 May 2005"}}], "et": [["text", "{dx_ety}see {dxt|rudiment||}{/dx_ety}"]], "date": "1619{ds||1||}", "shortdef": ["consisting in first principles : fundamental", "of a primitive kind", "very imperfectly developed or represented only by a vestige"]}]