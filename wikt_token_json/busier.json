[{"meta": {"id": "busy:1", "uuid": "63cf1b75-d79a-40b6-93e5-054373ef62e4", "sort": "020507000", "src": "collegiate", "section": "alpha", "stems": ["busier", "busiest", "busily", "busy"], "offensive": false}, "hom": 1, "hwi": {"hw": "busy", "prs": [{"mw": "\u02c8bi-z\u0113", "sound": {"audio": "busy0001", "ref": "c", "stat": "1"}}]}, "fl": "adjective", "ins": [{"if": "bus*i*er"}, {"ifc": "-est", "if": "bus*i*est"}], "def": [{"sseq": [[["sense", {"sn": "1 a", "dt": [["text", "{bc}engaged in action {bc}{sx|occupied||} "], ["vis", [{"t": "She's {wi}busy{/wi} studying."}, {"t": "has enough work to keep him {wi}busy{/wi} for a while"}]]]}], ["sense", {"sn": "b", "dt": [["text", "{bc}being in use "], ["vis", [{"t": "found the telephone {wi}busy{/wi}"}]]]}]], [["sense", {"sn": "2", "dt": [["text", "{bc}full of activity {bc}{sx|bustling||} "], ["vis", [{"t": "a {wi}busy{/wi} seaport"}]]]}]], [["sense", {"sn": "3", "dt": [["text", "{bc}foolishly or intrusively active {bc}{sx|meddling||} "], ["vis", [{"t": "a {wi}busy{/wi}, fussy sort of man much concerned with regulating everything", "aq": {"auth": "A. M. Young"}}]]]}]], [["sense", {"sn": "4", "dt": [["text", "{bc}full of distracting detail "], ["vis", [{"t": "a {wi}busy{/wi} design"}]]]}]]]}], "uros": [{"ure": "bus*i*ly", "prs": [{"mw": "\u02c8bi-z\u0259-l\u0113", "sound": {"audio": "busy0002", "ref": "c", "stat": "1"}}], "fl": "adverb"}], "syns": [{"pl": "synonyms", "pt": [["text", "{sc}busy{/sc} {sc}industrious{/sc} {sc}diligent{/sc} {sc}assiduous{/sc} {sc}sedulous{/sc} mean actively engaged or occupied. {sc}busy{/sc} chiefly stresses activity as opposed to idleness or leisure. "], ["vis", [{"t": "too {it}busy{/it} to spend time with the children"}]], ["text", " {sc}industrious{/sc} implies characteristic or habitual devotion to work. "], ["vis", [{"t": "{it}industrious{/it} employees"}]], ["text", " {sc}diligent{/sc} suggests earnest application to some specific object or pursuit. "], ["vis", [{"t": "very {it}diligent{/it