t": [["text", "{bc}be physically or mentally able to "], ["vis", [{"t": "He {wi}can{/wi} lift 200 pounds."}]]]}], ["sense", {"sn": "b", "dt": [["text", "{bc}know how to "], ["vis", [{"t": "She {wi}can{/wi} read."}]]]}], ["sense", {"sn": "c", "dt": [["uns", [[["text", "used to indicate possibility "], ["vis", [{"t": "Do you think he {wi}can{/wi} still be alive?"}, {"t": "Those things {wi}can{/wi} happen."}]]], [["text", "sometimes used interchangeably with {it}may{/it}"]]]]]}], ["sense", {"sn": "d", "dt": [["text", "{bc}be inherently able or designed to "], ["vis", [{"t": "everything that money {wi}can{/wi} buy"}]]]}], ["sense", {"sn": "e", "dt": [["text", "{bc}be enabled by law, agreement, or custom to "], ["vis", [{"t": "Congress {wi}can{/wi} declare war."}]]]}], ["sense", {"sn": "f", "dt": [["text", "{bc}be permitted by conscience or feeling to "], ["vis", [{"t": "{wi}can{/wi} hardly blame her"}]]]}], ["sense", {"sn": "g", "dt": [["text", "{bc}be made possible or probable by circumstances to "], ["vis", [{"t": "He {wi}can{/wi} hardly have meant that."}]]]}], ["sense", {"sn": "h", "dt": [["text", "{bc}be logically or axiologically able to "], ["vis", [{"t": "2 + 2 {wi}can{/wi} also be written 3 + 1."}]]]}]], [["sense", {"sn": "2", "dt": [["text", "{bc}have permission to "], ["uns", [[["text", "used interchangeably with {it}may{/it} "], ["vis", [{"t": "You {wi}can{/wi} go now if you like."}]]]]]]}]]]}, {"vd": "transitive verb", "sseq": [[["sense", {"sn": "1", "sls": ["archaic"], "dt": [["text", "{bc}to be able to do, make, or accomplish"]]}]], [["sense", {"sn": "2", "sls": ["obsolete"], "dt": [["text", "{bc}{sx|know||}, {sx|understand||}"]]}]]]}, {"vd": "intransitive verb", "sseq": [[["sense", {"sls": ["archaic"], "dt": [["text", "{bc}to have knowledge or skill"]]}]]]}], "usages": [{"pl": "{parahw}can{/parahw} vs. {parahw}may{/parahw}", "pt": [["text", "{it}Can{/it} and {it}may{/it} are most frequently interchangeable in uses denoting possibility; because the possibility of one's doing something may depend on another's acquiescence, they have also become interchangeable in the sense denoting permission. The use of {it}can{/it} to ask or grant permission has been common since the 19th century and is well established, although some commentators feel {it}may{/it} is more appropriate in formal contexts. {it}May{/it} is relatively rare in negative constructions ({it}mayn't{/it} is not common); {it}cannot{/it} and {it}can't{/it} are usual in such contexts."]]}], "et": [["text", "Middle English (1st & 3rd singular present indicative), from Old English; akin to Old High German {it}kan{/it} (1st & 3rd singular present indicative) know, am able, Old English {it}cn\u0101wan{/it} to know {ma}{mat|know|}{/ma}"]], "date": "before 12th century{ds|t|2||}", "shortdef": ["be physically or mentally able to", "know how to", "\u2014used to indicate possibility \u2014sometimes used interchangeably with may"]}, {"meta": {"id": "shitcan", "uuid": "2b4f4ba7-8392-42e6-b0d3-f0b6728dcbce", "sort": "193699000", "src": "collegiate", "section": "alpha", "stems": ["shitcan", "shitcans"], "offensive": true}, "hwi": {"hw": "shit*can", "prs": [{"mw": "\u02c8shit-\u02cckan", "sound": {"audio": "shitcan_1", "ref": "owl", "stat": "1"}}]}, "vrs": [{"vl": "or", "va": "shit-can"}], "fl": "verb", "ins": [{"if": "shit*canned"}, {"il": "or", "if": "shit-canned"}, {"if": "shit*can*ning"}, {"il": "or", "if": "shit-can*ning"}], "def": [{"vd": "transitive verb", "sseq": [[["sense", {"sn": "1", "sls": ["vulgar"], "dt": [["text", "{bc}to dismiss from a position {bc}{sx|fire||} "], ["vis", [{"t": "And most important, I want my drawings back before I get {wi}shitcanned{/wi}.", "aq": {"auth": "Faye Kellerman"}}, {"t": "This is not a party that {wi}shitcans{/wi} its leader for losing one election.", "aq": {"auth": "Pat Martin"}}]]]}]], [["sense", {"sn": "2", "sls": ["vulgar"], "dt": [["text", "{bc}to abandon or dispose of "], ["vis", [{"t": "The cited reasons for {wi}shit-canning