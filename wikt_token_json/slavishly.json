[{"meta": {"id": "slavish", "uuid": "bc44b115-cbe7-4712-8084-cc4a969cc457", "sort": "194471700", "src": "collegiate", "section": "alpha", "stems": ["slavish", "slavishly", "slavishness", "slavishnesses"], "offensive": false}, "hwi": {"hw": "slav*ish", "prs": [{"mw": "\u02c8sl\u0101-vish", "sound": {"audio": "slavis01", "ref": "c", "stat": "1"}}, {"l": "sometimes", "mw": "\u02c8sla-"}]}, "fl": "adjective", "def": [{"sseq": [[["sense", {"sn": "1 a", "dt": [["text", "{bc}of or characteristic of someone held in forced servitude"]], "sdsense": {"sd": "especially", "dt": [["text", "{bc}basely or abjectly servile"]]}}], ["sense", {"sn": "b", "sls": ["archaic"], "dt": [["text", "{bc}{sx|despicable||}, {sx|low||} "], ["vis", [{"t": "I have done with {wi}slavish{/wi} fear of disaster.", "aq": {"auth": "Charlotte Bront\u00eb"}}, {"t": "\u2026 in a sense it is infinitely reasonable \u2026. But there is such a thing as a mean infinity, a base and {wi}slavish{/wi} eternity.", "aq": {"auth": "G. K. Chesterton"}}]]]}]], [["sense", {"sn": "2", "sls": ["archaic"], "dt": [["text", "{bc}{sx|oppressive||}, {sx|tyrannical||} "], ["vis", [{"t": "No minion of despotism ever ventured to advance more {wi}slavish{/wi} doctrines \u2026", "aq": {"auth": "George McDuffie"}}]]]}]], [["sense", {"sn": "3", "sls": ["disapproving"], "dt": [["text", "{bc}copying obsequiously or without originality {bc}{sx|imitative||}"]]}]]]}], "uros": [{"ure": "slav*ish*ly", "fl": "adverb"}, {"ure": "slav*ish*ness", "fl": "noun"}], "date": "1565{ds||1|a|}", "shortdef": ["of or characteristic of someone held in forced servitude; especially : basely or abjectly servile", "despicable, low", "oppressive, tyrannical"]}]